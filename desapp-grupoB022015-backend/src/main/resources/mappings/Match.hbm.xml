<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-access="field"
	default-cascade="save-update">
	<class name="ar.edu.unq.desapp.grupoB022015.model.Match">
		<id column="ID" name="id">
			<generator class="native" />
		</id>
		
		<list name="local" inverse="false" cascade="all" lazy="false">
			<key column="local" />
			<list-index column="id" />
			<many-to-many class="ar.edu.unq.desapp.grupoB022015.model.User" unique="true"/>
		</list>
		
		<list name="visitor" inverse="false" cascade="all" lazy="false">
			<key column="visitor" />
			<list-index column="id" />
			<many-to-many class="ar.edu.unq.desapp.grupoB022015.model.User" unique="true"/>
		</list>
			
		<property name="localPoints" />
		
		<property name="visitorPoints" />
		
	</class>
</hibernate-mapping>
